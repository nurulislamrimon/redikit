{
  "name": "redikit",
  "version": "1.0.1",
  "description": "TypeScript version of lightweight Redis utility functions for caching and pub/sub",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "redis",
    "typescript",
    "cache",
    "pubsub",
    "redis-utils",
    "node-redis"
  ],
  "author": "Nurul Islam Rimon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nurulislamrimon/redikit.git"
  },
  "bugs": {
    "url": "https://github.com/nurulislamrimon/redikit/issues"
  },
  "homepage": "https://github.com/nurulislamrimon/redikit#readme",
  "dependencies": {
    "redis": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.50",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.50.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
